   "source": [
    "def mydecorator(func):\n",
    "    def wrapper(a,b):\n",
    "\n",
    "        func(a,b)\n",
    "\n",
    "    return wrapper\n",
    "@mydecorator\n",
    "def div(a, b):\n",
    "    try:\n",
    "        print( a / b)\n",
    "    except ZeroDivisionError:\n",
    "        print(\"Can't divide by zero\")\n",
    "\n",
    "div(6,2)\n",
    "\n",
    "div(3,0)"
   ],
